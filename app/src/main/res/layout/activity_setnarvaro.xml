<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true" >


    <ScrollView
        android:layout_width="wrap_content"
        android:paddingTop="60dp"
        android:layout_height="wrap_content">

        <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/MåndagEjNärvarandeView"
                android:layout_width="50dp"
                android:layout_height="30dp"
                android:layout_marginEnd="16dp"
                android:layout_marginTop="6dp"
                android:background="@android:color/darker_gray"
                android:onClick="clickEventMondagEjNärvarande"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView11" />

            <ImageView
                android:id="@+id/måndagNärvarandeView"
                android:layout_width="50dp"
                android:layout_height="30dp"
                android:layout_marginEnd="19dp"
                android:layout_marginTop="85dp"
                android:background="@android:color/darker_gray"
                android:onClick="clickEventMondagNärvarande"
                app:layout_constraintEnd_toStartOf="@+id/MåndagEjNärvarandeView"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/TisdagEjNärvarandeView"
                android:layout_width="50dp"
                android:layout_height="30dp"
                android:layout_marginEnd="16dp"
                android:layout_marginTop="36dp"
                android:background="@android:color/darker_gray"
                android:onClick="clickEventTisdagEjNärvarande"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/MåndagEjNärvarandeView" />

            <ImageView
                android:id="@+id/tisdagNärvarandeView"
                android:layout_width="50dp"
                android:layout_height="30dp"
                android:layout_marginEnd="16dp"
                android:layout_marginTop="32dp"
                android:background="@android:color/darker_gray"
                android:onClick="clickEventTisdagNärvarande"
                app:layout_constraintEnd_toStartOf="@+id/TisdagEjNärvarandeView"
                app:layout_constraintTop_toBottomOf="@+id/måndagNärvarandeView" />

            <ImageView
                android:id="@+id/OnsdagEjNärvarandeView"
                android:layout_width="50dp"
                android:layout_height="30dp"
                android:layout_marginEnd="16dp"
                android:layout_marginTop="33dp"
                android:background="@android:color/darker_gray"
                android:onClick="clickEventOnsdagEjNärvarande"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/TisdagEjNärvarandeView" />

            <ImageView
                android:id="@+id/onsdagNärvarandeView"
                android:layout_width="50dp"
                android:layout_height="30dp"
                android:layout_marginEnd="19dp"
                android:layout_marginTop="34dp"
                android:background="@android:color/darker_gray"
                android:onClick="clickEventOnsdagNärvarande"
                app:layout_constraintEnd_toStartOf="@+id/OnsdagEjNärvarandeView"
                app:layout_constraintTop_toBottomOf="@+id/tisdagNärvarandeView" />

            <ImageView
                android:id="@+id/TorsdagEjNärvarandeView"
                android:layout_width="50dp"
                android:layout_height="30dp"
                android:layout_marginEnd="16dp"
                android:layout_marginTop="34dp"
                android:background="@android:color/darker_gray"
                android:onClick="clickEventTorsdagEjNärvarande"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/OnsdagEjNärvarandeView" />

            <ImageView
                android:id="@+id/torsdagNärvarandeView"
                android:layout_width="50dp"
                android:layout_height="30dp"
                android:layout_marginEnd="19dp"
                android:layout_marginTop="34dp"
                android:background="@android:color/darker_gray"
                android:onClick="clickEventTorsdagNärvarande"
                app:layout_constraintEnd_toStartOf="@+id/TorsdagEjNärvarandeView"
                app:layout_constraintTop_toBottomOf="@+id/onsdagNärvarandeView" />

            <ImageView
                android:id="@+id/FredagEjNärvarandeView"
                android:layout_width="50dp"
                android:layout_height="30dp"
                android:layout_marginEnd="16dp"
                android:layout_marginTop="34dp"
                android:background="@android:color/darker_gray"
                android:onClick="clickEventFredagEjNärvarande"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/TorsdagEjNärvarandeView" />

            <ImageView
                android:id="@+id/FredagNärvarandeView"
                android:layout_width="50dp"
                android:layout_height="30dp"
                android:layout_marginEnd="19dp"
                android:layout_marginTop="35dp"
                android:background="@android:color/darker_gray"
                android:onClick="clickEventFredagNärvarande"
                app:layout_constraintEnd_toStartOf="@+id/FredagEjNärvarandeView"
                app:layout_constraintTop_toBottomOf="@+id/torsdagNärvarandeView" />

            <Button
                android:id="@+id/buttonClose"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="16dp"
                android:layout_marginTop="104dp"
                android:onClick="onClickClose"
                android:text="Close"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/FredagNärvarandeView" />

            <Spinner
                android:id="@+id/spinner3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <Spinner
                android:id="@+id/UserSpinner"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="42dp"
                android:layout_marginTop="16dp"
                app:layout_constraintStart_toEndOf="@+id/spinner3"
                app:layout_constraintTop_toTopOf="parent" />

            <Spinner
                android:id="@+id/spinner2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="36dp"
                android:layout_marginTop="16dp"
                app:layout_constraintStart_toEndOf="@+id/UserSpinner"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="47dp"
                android:text="Måndag"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/spinner3" />

            <TextView
                android:id="@+id/textView13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="45dp"
                android:text="Tisdag"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView12" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="45dp"
                android:text="Onsdag"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView13" />

            <TextView
                android:id="@+id/textView16"
                android:layout_width="wrap_content"
                android:layout_height="19dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="46dp"
                android:text="Torsdag"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView14" />

            <TextView
                android:id="@+id/textView15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="45dp"
                android:text="Fredag"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView16" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="41dp"
                android:text="startdag"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="slutdag"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView10" />

        </android.support.constraint.ConstraintLayout>

    </ScrollView>

    <FrameLayout
        android:id="@+id/content_frame"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <android.support.v7.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="?android:attr/colorActivatedHighlight"
            android:theme="@style/ThemeOverlay.AppCompat.ActionBar" />

    </FrameLayout>


    <android.support.design.widget.NavigationView
        android:id="@+id/nav_view"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:fitsSystemWindows="true"
        app:menu="@menu/drawer_view"
        app:headerLayout="@layout/nav_header" />
</android.support.v4.widget.DrawerLayout>
